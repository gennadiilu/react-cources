{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "styled-components",
          {
            "ssr": false,
            "minify": false,
            "displayName": true
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        [
          "styled-components",
          {
            "ssr": false,
            "minify": true,
            "displayName": false
          }
        ]
      ]
    }
  }
}
